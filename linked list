import java.util.Scanner;
class Node{
  int val;
 
  Node next;
  Node(int val){
    this.val=val;
    this.next=null;
  }
  
}


class LinkedList{
       Node head=null;
       Node tail=null;
       void insert(int ele){
         Node node=new Node(ele);
         if(head==null){
           head=node;
           tail=node;
         }
         else{
           tail.next=node;
           tail=node;
         }
       }
      void display(){
        Node temp=head;
        while(temp!=null){
          System.out.print(temp.val+" ");
          temp=temp.next;
        }
      }}
 class Main{
  public static void main(String[] args){
    Scanner sc=new Scanner(System.in);
    LinkedList ll=new LinkedList();
    int n=sc.nextInt();
    int ele;
    for(int i=0;i<n;i++){
      ele=sc.nextInt();
     ll.insert(ele);
    }
    ll.display();
  }
}
